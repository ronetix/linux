/dts-v1/;

#include "fsl-imx8mq.dtsi"

/ {
	model = "Ronetix i.MX8MQ-CM";
	compatible = "fsl,imx8mq-evk", "fsl,imx8mq";

	memory@40000000 {
		device_type = "memory";
		reg = <0x00000000 0x40000000 0 0x40000000>;		/* 1 GB */
	};
	
	resmem: reserved-memory {
		linux,cma {
			/* 
			 * 0x04000000 -  64 MB 
			 * 0x10000000 - 256 MB 
			 * 0x18000000 - 384 MB, when CSI is used 
			 */
			size = <0 0x18000000>;	
		};
	};
	        
	chosen {
		bootargs = "console=ttymxc0,115200 earlycon=ec_imx6q,0x30860000,115200";
		stdout-path = &uart1;
	};

	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		reg_usdhc2_vmmc: usdhc2_vmmc {
			compatible = "regulator-fixed";
			regulator-name = "VSD_3V3";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			gpio = <&gpio2 19 GPIO_ACTIVE_HIGH>;
			off-on-delay = <20000>;
			enable-active-high;
		};

		reg_gpio_dvfs: regulator-gpio {
			compatible = "regulator-gpio";
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_dvfs>;
			regulator-min-microvolt = <900000>;
			regulator-max-microvolt = <1000000>;
			regulator-name = "gpio_dvfs";
			regulator-type = "voltage";
			gpios = <&gpio3 12 GPIO_ACTIVE_HIGH>;
			states = <900000 0x1 1000000 0x0>;
		};
		
		reg_wifi: reg_wifi {
			compatible = "regulator-fixed";
			regulator-name = "WIFI_REG_ON";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			gpio = <&gpio3 4 GPIO_ACTIVE_HIGH>;		/* WIFI_PMU_ENABLE, Open-Drain */
			enable-active-high;
			gpio-open-drain;
		};
	};

