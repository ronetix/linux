				remote-endpoint = <&mipi1_sensor_ep>;
			};
		};
	};
};

&i2c2 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
	status = "okay";

	i2c_eeprom: i2c_eeprom@50 {
        compatible = "atmel,24c512";
        reg = <0x50>;
        pagesize = <128>;
    };

	wm8731: wm8731@1b {
		#sound-dai-cells = <0>;
		compatible = "wlf,wm8731";
		reg = <0x1B>;
		status = "okay";
	};
	
	pmic: pfuze100@08 {
		compatible = "fsl,pfuze100";
		reg = <0x08>;

		regulators {
			sw1a_reg: sw1ab {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
			};

			sw1c_reg: sw1c {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
			};

			sw2_reg: sw2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			sw3a_reg: sw3ab {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-always-on;
			};

			sw4_reg: sw4 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			swbst_reg: swbst {
				regulator-min-microvolt = <5000000>;
				regulator-max-microvolt = <5150000>;
			};

			snvs_reg: vsnvs {
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <3000000>;
				regulator-always-on;
			};

			vref_reg: vrefddr {
				regulator-always-on;
			};

			vgen1_reg: vgen1 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen2_reg: vgen2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
				regulator-always-on;
			};

			vgen3_reg: vgen3 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen4_reg: vgen4 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen5_reg: vgen5 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen6_reg: vgen6 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
			};
		};
	};

	dsi_lvds_bridge: sn65dsi83@2c {
		compatible = "ti,sn65dsi83";
		reg = <0x2c>;
		ti,dsi-lanes = <4>;
		ti,lvds-format = <1>;
		ti,lvds-bpp = <24>;
		ti,width-mm = <152>;
		ti,height-mm = <91>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_lvds>;
		enable-gpios = <&gpio3 10 GPIO_ACTIVE_HIGH>;
		status = "disabled";
#if 0		
		enable-test-pattern;
#endif		
		
		display-timings {
			/* InnoLux G070Y2-L01 - doesn't work because of
			 * display pixel clock driver limitation to 74.25 MHz
			 * Next PCB revision should implement a 10" 1280x800 display
			 */
			lvds {
				clock-frequency = <29500000>;
				hactive = <800>;
				vactive = <480>;
				
				hfront-porch 	= <91>;	/* 91, 16, 56 */
				hback-porch	 	= <90>; /* 90, 96  96 */
				hsync-len 		= <12>; /* 12, 80  40 */

				vfront-porch 	= <9>; 	/* 9        4*/
				vback-porch 	= <8>;	/* 8       12 */
				vsync-len 		= <2>;  /* 2        4 */
				
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <1>;
				pixelclk-active = <0>;
			};
		};

		port {
			sn65dsi83_in: endpoint {
				remote-endpoint = <&mipi_dsi_bridge_out>;
			};
		};
	};
};

