				MX8MQ_IOMUXC_GPIO1_IO00_ANAMIX_REF_CLK_32K	0x16	/* REF_CLK_32K */
			>;
		};

		pinctrl_wlan_init: wlan_initgrp {
			fsl,pins = <
				MX8MQ_IOMUXC_GPIO1_IO00_GPIO1_IO0		0x16	/* REF_CLK_32K */
				MX8MQ_IOMUXC_NAND_CE3_B_GPIO3_IO4		0x36	/* WIFI_PMU_ENABLE, Open-Drain */
				MX8MQ_IOMUXC_NAND_CLE_GPIO3_IO5			0x16	/* WIFI_PWR_DOWN */
			>;
		};

		pinctrl_backlight: backlightgrp {
			fsl,pins = <
					MX8MQ_IOMUXC_GPIO1_IO03_GPIO1_IO3  0x16 	/* LCD_BL_EN */
			>;
		};

		pinctrl_pwm1: pwm1grp {
			fsl,pins = <
				MX8MQ_IOMUXC_GPIO1_IO01_PWM1_OUT 		0x06
			>;
		};

		pinctrl_lvds: lvdsgrp {
			fsl,pins = <
				MX8MQ_IOMUXC_NAND_DATA04_GPIO3_IO10		0x16	/* SN65DSI83 - chip enable */
				MX8MQ_IOMUXC_SAI3_MCLK_GPIO5_IO2		0x16	/* panel enable - just for test */
			>;
		};
	};
};

